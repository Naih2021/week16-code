pipeline {
    agent any 
    tools {
        maven 'M2_HOME'
    }
    stages {
        stage('sonarqube scan'){
            steps{
                withSonarQubeEnv('sonarserver'){
                sh 'mvn verify org.sonarsource.scanner.maven:sonar-maven-plugin:sonar -Dsonar.projectKey=Naih2021_week16-code'
                }
                
            }
        }
        stage('all maven commands') {
            steps {
                sh 'mvn clean test compile install package'
            }
        }
        stage('upload artifact'){
            steps{
               sh 'curl -uadmin:APcc1oHerkZ5F3P4NVmHNAjYUz -T \
               target/bio*.jar \
               "http://54.86.132.223:8081/artifactory/geoapp/"'
            }
        }
    }
}
